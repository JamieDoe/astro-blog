---
import Hamburger from './Hamburger.astro'
import Navigation from './Navigation.astro'
import ThemeIcon from './ThemeIcon.astro'

const navLinks = [
  {
    path: '/',
    label: 'Home'
  },
  {
    path: '/about',
    label: 'About'
  },
  {
    path: '/blog',
    label: 'Blog'
  },
  {
    path: '/tags',
    label: 'Tags'
  }
]
---

<header>
  <Hamburger navLinks={navLinks} />
  <ThemeIcon />
  <Navigation navLinks={navLinks}/>
</header>
